{
  "name": "cdktf-azure-providers",
  "author": "Cyrus Wong",
  "version": "0.0.12",
  "repository": {
    "url": "https://github.com/wongcyrus/AzureConstructLib",
    "type": "remote"
  },
  "main": "patterns/index.js",
  "types": "patterns/index.ts",
  "license": "MPL-2.0",
  "keywords": [
    "cdk-tf",
    "azure"
  ],
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "tsc && jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "build-jsii": "jsii",
    "build-jsii:watch": "jsii -w",
    "package-jsii": "jsii-pacmak"
  },
  "engines": {
    "node": ">=14.0"
  },
  "peerDependencies": {
    "cdktf": "^0.20.7",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/prettier": "3.0.0",
    "cdktf": "^0.21.0",
    "cdktf-cli": "^0.21.0",
    "constructs": "^10.4.2",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "jsii": {
    "excludeTypescript": [
      "*.d.ts",
      "cdktf.out"
    ],
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "azure-common-construct",
        "module": "azure-common-construct.base"
      },
      "java": {
        "package": "com.cloudlabhk.azurecommoncontruct",
        "maven": {
          "groupId": "com.cloudlabhk.azurecommoncontruct",
          "artifactId": "azure-common-construct"
        }
      },
      "dotnet": {
        "namespace": "AzureCommonConstruct.Jsii",
        "packageId": "AzureCommonConstruct.Jsii"
      }
    }
  }
}
